<?xml version="1.0" encoding="ISO-8859-1"?>
<chains>
	<catalog>
		
		<command name="lsitems"
			className="org.apache.jackrabbit.chain.command.info.LsItems"
			defaultMaxItems="100"
			maxItemsKey="maxitems"
			patternKey="pattern"/>

		<chain name="lscollect">
			<command 
				className="org.apache.jackrabbit.chain.command.CollectItems" 
				depthKey="depth" 
				namePatternKey="pattern" 
				toKey="collected" />
			<command 
				className="org.apache.jackrabbit.chain.command.info.LsCollectedItems"
				fromKey="collected"
				maxItemsKey="maxitems"/>
			<command
				className="org.apache.commons.chain.generic.RemoveCommand"
				fromKey="collected"/>
		</chain>
		
		<command name="lsnodes"
			className="org.apache.jackrabbit.chain.command.info.LsNodes"
			maxItemsKey="maxitems"
			patternKey="pattern"
			pathKey="path"
			uuidKey="uuid"
			mixinKey="m"
			nodesSizeKey="nsize"
			propertiesSizeKey="psize"
			referencesSizeKey="rsize"
			versionableKey="v"
			lockableKey="l"
			referenceableKey="r"
			lockedKey="locked"
			hasLockKey="hasLock"
			new_Key="new"
			modifiedKey="mod"/>
						
		<chain name="lscollectnodes">
			<command 
				className="org.apache.jackrabbit.chain.command.CollectNodes" 
				depthKey="depth" 
				namePatternKey="pattern" 
				toKey="collected" />
			<command 
				className="org.apache.jackrabbit.chain.command.info.LsCollectedNodes"
				fromKey="collected"
				maxItemsKey="maxitems"
				pathKey="path"
				uuidKey="uuid"
				mixinKey="m"
				nodesSizeKey="nsize"
				propertiesSizeKey="psize"
				referencesSizeKey="rsize"
				versionableKey="v"
				lockableKey="l"
				referenceableKey="r"
				lockedKey="locked"
				hasLockKey="hasLock"
				new_Key="new"
				modifiedKey="mod"
				/>
			<command
				className="org.apache.commons.chain.generic.RemoveCommand"
				fromKey="collected"/>
		</chain>
		
		<command name="lsproperties"
			className="org.apache.jackrabbit.chain.command.info.LsProperties"
			maxItemsKey="maxitems"
			patternKey="pattern"/>

		<chain name="lscollectproperties">
			<command 
				className="org.apache.jackrabbit.chain.command.CollectProperties" 
				depthKey="depth" 
				namePatternKey="pattern" 
				toKey="collected" />
			<command 
				className="org.apache.jackrabbit.chain.command.info.LsCollectedProperties"
				fromKey="collected"
				maxItemsKey="maxitems"/>
			<command
				className="org.apache.commons.chain.generic.RemoveCommand"
				fromKey="collected"/>
		</chain>

		<chain name="sqlquery">
			<command 
				className="org.apache.jackrabbit.chain.command.query.SQLQuery"
				statementKey="statement"
				toKey="collected"/>
			<command 
				className="org.apache.jackrabbit.chain.command.info.LsCollectedNodes"
				fromKey="collected"
				maxItemsKey="maxitems"
				pathKey="path"
				uuidKey="uuid"
				mixinKey="m"
				nodesSizeKey="nsize"
				propertiesSizeKey="psize"
				referencesSizeKey="rsize"
				versionableKey="v"
				lockableKey="l"
				referenceableKey="r"
				lockedKey="locked"
				hasLockKey="hasLock"
				new_Key="new"
				modifiedKey="mod"
				/>
		</chain>

		<chain name="xpathquery">
			<command 
				className="org.apache.jackrabbit.chain.command.query.XPathQuery"
				statementKey="statement"
				toKey="collected"/>
			<command 
				className="org.apache.jackrabbit.chain.command.info.LsCollectedNodes"
				fromKey="collected"
				maxItemsKey="maxitems"
				pathKey="path"
				uuidKey="uuid"
				mixinKey="m"
				nodesSizeKey="nsize"
				propertiesSizeKey="psize"
				referencesSizeKey="rsize"
				versionableKey="v"
				lockableKey="l"
				referenceableKey="r"
				lockedKey="locked"
				hasLockKey="hasLock"
				new_Key="new"
				modifiedKey="mod"
				/>
		</chain>					
	</catalog>
</chains>